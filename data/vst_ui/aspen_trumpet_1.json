{
  "title": "Aspen Trumpet 1",
  "subtitle": "Expressive orchestral brass",
  "artwork": {
    "type": "svg",
    "path": "aspen_trumpet_1.svg"
  },
  "theme": {
    "background": "#0f172a",
    "accent": "#f97316",
    "accentSecondary": "#1e293b",
    "text": "#f8fafc",
    "panel": "#14213d",
    "panelStroke": "#f97316"
  },
  "keyboard": {
    "min_note": 48,
    "max_note": 84
  },
  "panels": [
    {
      "title": "Envelope",
      "layout": "grid",
      "controls": [
        {"param": "attack", "type": "knob", "label": "Attack", "min": 0.01, "max": 1.5},
        {"param": "decay", "type": "knob", "label": "Decay", "min": 0.05, "max": 1.2},
        {"param": "sustain", "type": "knob", "label": "Sustain", "min": 0.2, "max": 1.0},
        {"param": "release", "type": "knob", "label": "Release", "min": 0.05, "max": 2.0}
      ]
    },
    {
      "title": "Expression",
      "layout": "grid",
      "controls": [
        {"param": "vibratoRate", "type": "slider", "label": "Vibrato Rate", "min": 0.5, "max": 8.0},
        {"param": "vibratoDepth", "type": "slider", "label": "Vibrato Depth", "min": 0.0, "max": 0.8},
        {"param": "brightness", "type": "slider", "label": "Brightness", "min": 0.0, "max": 1.0},
        {"param": "breath", "type": "slider", "label": "Breath", "min": 0.0, "max": 0.6}
      ]
    },
    {
      "title": "Stage",
      "layout": "row",
      "controls": [
        {"param": "expression", "type": "slider", "label": "Expression", "min": 0.2, "max": 1.5},
        {"param": "hallMix", "type": "slider", "label": "Hall Mix", "min": 0.0, "max": 0.9}
      ]
    }
  ]
}
